{
  "name": "gohan-webui",
  "version": "1.16.3",
  "description": "WebUI for Gohan project",
  "main": "./app/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register --recursive -r mock-local-storage -r jsdom-global/register",
    "test-watch": "npm test -- --watch",
    "test-cover": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover --report text --report html ./node_modules/.bin/_mocha -- --recursive -r mock-local-storage  -r jsdom-global/register",
    "build": "rm -rf ./dist/ && NODE_ENV=production ./node_modules/.bin/webpack --colors --progress --bail",
    "build-dev": "rm -rf ./dist/ && ./node_modules/.bin/webpack -d --colors --display-reasons --progress --bail",
    "dev-server": "rm -rf ./dist/ && ./node_modules/.bin/webpack-dev-server -d --inline --colors --progress",
    "lint": "./node_modules/.bin/eslint `find ./app/ -name '*.js'`"
  },
