{
  "name": "gohan-webui",
  "version": "2.0.0",
  "description": "WebUI for Gohan project",
  "main": "./src/index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register --recursive",
    "test-watch": "npm test -- --watch",
    "test-cover": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover --report text --report html ./node_modules/.bin/_mocha -- --recursive",
    "build": "rm -rf ./dist/ && ./node_modules/.bin/webpack -p --colors --progress --bail",
    "build-dev": "rm -rf ./dist/ && ./node_modules/.bin/webpack -d --colors --display-reasons --progress --bail",
    "dev-server": "rm -rf ./dist/ && ./node_modules/.bin/webpack-dev-server -d --hot --inline --colors --progress",
    "lint": "./node_modules/.bin/eslint `find ./src/ -regextype sed -regex '.*.jsx\\|.*.js'`"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cloudwan/gohan_webui.git"
  },
  "author": "",
  "contributors": [
