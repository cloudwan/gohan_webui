    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "mocha": "^2.3.4",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-jsonschema-form": "^0.40.0",
