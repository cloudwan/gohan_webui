    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "bower-webpack-plugin": "^0.1.9",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.6.1",
    "chai-spies": "^0.7.1",
    "copy-webpack-plugin": "^0.2.0",
    "css-loader": "^0.22.0",
    "debug": "git+https://github.com/stepek/debug.git",
