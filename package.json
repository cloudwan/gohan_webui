{
  "name": "gohan-webui",
  "version": "2.6.0",
  "description": "WebUI for Gohan project",
  "main": "./src/index.js",
  "scripts": {
    "postinstall": "if [ -a .git ]; then ln -s -f ./../../.hooks/pre-commit ./.git/hooks/pre-commit; fi",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha `find ./src/ -regextype sed -regex '.*.test.jsx\\|.*.test.js'`",
    "test-watch": "npm test -- --watch",
    "test-cover": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- `find ./src/ -regextype sed -regex '.*.test.jsx\\|.*.test.js'`",
    "build": "rm -rf ./dist/ && NODE_ENV=production ./node_modules/.bin/webpack --colors --progress --bail",
    "build-dev": "rm -rf NODE_ENV=development ./dist/ && ./node_modules/.bin/webpack -d --colors --display-reasons --progress --bail",
    "dev-server": "rm -rf ./dist/ && NODE_ENV=dev-server ./node_modules/.bin/webpack-dev-server --hot --inline --colors --progress",
    "lint": "./node_modules/.bin/eslint `find ./src/ -regextype sed -regex '.*.jsx\\|.*.js'`",
    "doc": "./node_modules/.bin/esdoc -c .esdoc.json"
  },
