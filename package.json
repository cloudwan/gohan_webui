{
  "name": "gohan-webui",
  "version": "2.0.0",
  "description": "WebUI for Gohan project",
  "main": "./src/index.js",
  "scripts": {
    "postinstall": "ln -s -f ./../../.hooks/pre-commit ./.git/hooks/pre-commit",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-register --compilers jsx:babel-register --require babel-polyfill --recursive ./test/helpers/dom.js ./test/helpers/material.js `find ./src/ -regextype sed -regex '.*.test.jsx\\|.*.test.js'`",
    "test-watch": "npm test -- --watch",
    "test-cover": "NODE_ENV=test ./node_modules/.bin/istanbul cover --include-all-sources -x *.test.js -x *.test.jsx --report text --report html ./node_modules/.bin/_mocha -- --compilers js:babel-register --compilers jsx:babel-register  --require babel-polyfill  --recursive ./test/helpers/dom.js ./test/helpers/material.js `find ./src/ -regextype sed -regex '.*.test.jsx\\|.*.test.js'`",
    "build": "rm -rf ./dist/ && NODE_ENV=production ./node_modules/.bin/webpack -p --colors --progress --bail",
    "build-dev": "rm -rf NODE_ENV=development ./dist/ && ./node_modules/.bin/webpack -d --colors --display-reasons --progress --bail",
    "dev-server": "rm -rf ./dist/ && NODE_ENV=development ./node_modules/.bin/webpack-dev-server -d --hot --inline --colors --progress",
    "lint": "./node_modules/.bin/eslint `find ./src/ -regextype sed -regex '.*.jsx\\|.*.js'`"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cloudwan/gohan_webui.git"
  },
  "author": "",
  "contributors": [
    {
      "name": "Nachi Ueno",
      "email": "nati.ueno@gmail.com"
    },
    {
      "name": "Toshiyuki Hayashi",
      "email": "hayashi@ntti3.com"
    },
    {
      "name": "Kamil Stepczuk",
      "email": "k.stepczuk@hotmail.com"
    },
    {
      "name": "Adam Baszak",
      "email": "adambaszak@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
