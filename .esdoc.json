{
  "source": "./src",
  "destination": "./doc",
  "includes": ["\\.(js|jsx)$"],
  "excludes": ["\\.test\\.(js|jsx)$"],
  "autoPrivate": true,
  "index": "./README.md",
  "package": "./package.json",
  "plugins": [{
    "name": "esdoc-flow-plugin"
  }],
  "experimentalProposal": {
    "classProperties": true,
    "objectRestSpread": true
  }
}
